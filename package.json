    "babel-jest": "^29.0.2",
    "eslint": "^8.23.0",
    "eslint-config-universe": "^11.0.0",
    "jest": "^29.0.2",
    "metro-config": "^0.67.0",
    "metro-react-native-babel-preset": "^0.68.0",
    "prettier": "^2.7.1",
